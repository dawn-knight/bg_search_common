package com.mbgo.search.core.bean.index;

import java.io.Serializable;

public class TagInfo implements Serializable {

	private static final long serialVersionUID = -5785422299825280725L;
	private String name;
	private int sort;
	public TagInfo() {
	}
	
	public TagInfo(String name, int sort) {
		super();
		this.name = name;
		this.sort = sort;
	}

	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getSort() {
		return sort;
	}
	public void setSort(int sort) {
		this.sort = sort;
	}
	
}
